server {
    server_name phoebesnotebook.com www.phoebesnotebook.com;

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/mysite/mysite.sock;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/phoebesnotebook.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/phoebesnotebook.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}server {
    if ($host = www.phoebesnotebook.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = phoebesnotebook.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name phoebesnotebook.com www.phoebesnotebook.com;
    return 404; # managed by Certbot




}